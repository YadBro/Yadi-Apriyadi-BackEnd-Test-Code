  },
  "homepage": "https://github.com/YadBro/Yadi-Apriyadi-BackEnd-Test-Code#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.0",
    "sequelize": "^6.29.3"
  },
