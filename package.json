{
  "name": "coding-test-pt-informatika-media-pratama",
  "version": "1.0.0",
  "description": "This is restful api user for coding test PT INFORMATIKA MEDIA PRATAMA",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "model:generate:user": "sequelize-cli model:generate --name user --attributes username:string,password:string,fullname:string",
    "seed:generate:user": "sequelize-cli seed:generate --name user",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:down": "sequelize-cli db:migrate:undo:all",
    "seed:up": "sequelize-cli db:seed:all",
    "seed:down": "sequelize-cli db:seed:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YadBro/Yadi-Apriyadi-BackEnd-Test-Code.git"
  },
